
import os, sys
import glob
import numpy as np

from astropy.time import Time

gpstime = 1238782701
skymap = "../data/GW190408/bayestar.fits"
cl = 0.9

outputDir = "../output/GW190408/%.3f_Nickel"%cl
system_command = "python gwemopt_run --telescope Nickel --doSchedule --doSkymap --doPlots --skymap %s --gpstime %d -o %s --doTiles --doSingleExposure --filters g --exposuretimes 30.0 --powerlaw_cl %.3f"%(skymap,gpstime,outputDir,cl)
print(system_command)


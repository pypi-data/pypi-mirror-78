
import os, sys
import numpy as np
from astropy import table
import astropy.units as u
from astropy.coordinates import SkyCoord

filename = "list.txt"
lines = [line.rstrip('\n') for line in open(filename)]
lines = lines[0].split(' ')
fieldlist = []
for line in lines:
    lineSplit = line.split(",")
    field = int(lineSplit[2])
    fieldlist.append(field)

filename = "list2.txt"
lines = [line.rstrip('\n') for line in open(filename)]
fieldlist2 = []
for line in lines:
    lineSplit = line.split(",")
    field = int(lineSplit[2])
    fieldlist2.append(field)

filename = "list3.txt"
lines = [line.rstrip('\n') for line in open(filename)]
lines = lines[0].split(' ')
fieldlist3 = []
for line in lines:
    lineSplit = line.split(",")
    field = int(lineSplit[2])
    fieldlist3.append(field)

night1 = fieldlist + fieldlist2 

#fielddiff = np.setdiff1d(fieldlist,fieldlist2)
print(night1)
print(fieldlist3)
fielddiff = np.setdiff1d(fieldlist3,night1)
print(fielddiff)

print(fieldlist3)
print(len(fieldlist3))

         * building your own nodes!
         */

    },
    "implementation_config": {
        "reader_config": {
            "my_reader": {
                "class": "AwesomeReader",
                "data_to_read": "Read me.",
                "destinations": [
                    "my_model"
                ]
            }
        },
        "model_config": {
            "my_model":{
                "class": "AwesomeModel",
                "mode": "train",
                "destinations": [
                ]
            }
        }
        }
}

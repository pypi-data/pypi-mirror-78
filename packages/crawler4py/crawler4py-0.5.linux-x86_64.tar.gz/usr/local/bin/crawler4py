#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'crawler4py==0.5','console_scripts','crawler4py'
__requires__ = 'crawler4py==0.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('crawler4py==0.5', 'console_scripts', 'crawler4py')()
    )

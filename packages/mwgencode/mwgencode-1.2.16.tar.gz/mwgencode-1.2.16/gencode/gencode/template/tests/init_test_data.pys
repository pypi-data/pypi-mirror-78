from app import db
class User:
    def __init__(self,uid,uname,systemuser,manageuser,manageuserid=None):
        self.uid = uid
        self.uname = uname
        self.systemuser = systemuser
        self.manageuser =manageuser
        self.manageuserid=manageuserid

    def to_json(self):
        return {"uid":self.uid,
                "uname":self.uname,
                "systemuser":self.systemuser,
                'manageuser':self.manageuser,
                'manageuserid':self.manageuserid
                }
    def __repr__(self):
        return "user(id:%s),username:%s,issystemuser:%s"%(self.uid,self.uname,self.systemuser)

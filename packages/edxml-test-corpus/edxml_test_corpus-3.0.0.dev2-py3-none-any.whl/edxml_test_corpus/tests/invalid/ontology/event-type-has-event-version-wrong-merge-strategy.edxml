<?xml version="1.0" encoding="UTF-8"?>
<!--
Testing event structure validation, verify
if an event type that defines an event version using
a property that does not have the max merge strategy
is rejected
-->
<edxml xmlns="http://edxml.org/edxml" version="3.0.0">
  <ontology>
    <objecttypes>
      <objecttype name="a" description="a" display-name-singular="a" display-name-plural="a" version="1" data-type="sequence"/>
    </objecttypes>
    <concepts/>
    <eventtypes>
      <eventtype name="a" description="a" event-version="c" display-name-singular="a" display-name-plural="a" summary="a" story="a" version="1">
        <properties>
          <property name="a" object-type="a" description="c" unique="true" optional="false" multivalued="false" merge="match"/>
          <property name="b" object-type="a" description="c" unique="false" optional="false" multivalued="false" merge="replace"/>
          <property name="c" object-type="a" description="c" unique="false" optional="false" multivalued="false" merge="min"/>
        </properties>
        <relations/>
      </eventtype>
    </eventtypes>
    <sources/>
  </ontology>
</edxml>

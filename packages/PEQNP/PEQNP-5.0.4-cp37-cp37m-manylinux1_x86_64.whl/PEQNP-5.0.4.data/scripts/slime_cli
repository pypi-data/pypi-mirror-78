#!python
import sys
import peqnp as pn


def help():
    print('usage: slime_cli <cnf> [model] [proof]')
    exit(0)


if __name__ == '__main__':
    if '-help' in sys.argv[1]:
        help()

    try:
        if len(sys.argv) == 2:
            pn.slime_cli(sys.argv[1])
        if len(sys.argv) == 3:
            pn.slime_cli(sys.argv[1], sys.argv[2])
        if len(sys.argv) == 4:
            pn.slime_cli(sys.argv[1], sys.argv[2], sys.argv[3])
    except Exception as ex:
        print(ex)
        help()
